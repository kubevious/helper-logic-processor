  "description": "Backend Logic Processor",
  "main": "dist/index.js",
  "types": "dist",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/*.ts' 'test/3rd-party/*.ts'",
    "dev-large-cluster": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/dev/dev-large-cluster.ts'",
    "dev-openshift": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/dev/dev-openshift.ts'",
    "build": "tsc",
    "format": "prettier --write ./src/ ./test/",
    "format-check": "prettier --write ./src/ ./test/",
    "lint": "eslint"
  },
  "author": "Ruben Hakopian <ruben.hakopian@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@kubevious/entity-meta": "^1.0.78",
    "@kubevious/helper-backend": "^1.0.38",
    "@kubevious/state-registry": "^1.0.13",
    "glob": "^7.1.7",
    "glob-promise": "^4.2.0",
    "kubernetes-types": "^1.21.0-beta.1",
    "moment": "^2.29.3",
    "the-lodash": "^2.0.9",
    "the-logger": "^2.0.14",
    "the-promise": "^2.0.15"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/js-yaml": "^4.0.2",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@types/should": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "js-yaml": "^4.1.0",
    "mocha": "^8.1.1",
    "prettier": "^2.1.0",
    "should": "^13.2.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "resolutions": {
    "the-lodash": "^2.0.9",
    "the-logger": "^2.0.14",
    "the-promise": "^2.0.15",
    "@kubevious/state-registry": "^1.0.13",
